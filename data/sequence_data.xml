<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Secuencia para Órdenes de Servicio -->
        <record id="seq_purchase_order_service" model="ir.sequence">
            <field name="name">Orden de Servicio</field>
            <field name="code">purchase.order.service</field>
            <field name="prefix">OS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Secuencia para Órdenes de Distribución -->
        <record id="seq_purchase_order_distribution" model="ir.sequence">
            <field name="name">Orden de Distribución</field>
            <field name="code">purchase.order.distribution</field>
            <field name="prefix">OD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>